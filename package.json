{
  "name": "@aldabil/react-scheduler",
  "version": "2.3.10",
  "description": "React scheduler component based on Material-UI & date-fns",
  "files": [
    "*"
  ],
  "scripts": {
    "start": "react-scripts start",
    "webpack": "webpack",
    "pack": "rm -rf dist && npm run webpack && npm run post:pack",
    "local:pack": "npm run pack && cd dist && npm pack && mv *.tgz ../",
    "post:pack": "node ./scripts/post-pack.js",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "pre:commit": "lint-staged"
  },
  "lint-staged": {
    "**/**.{ts,tsx}": [
      "tsc --noEmit"
    ],
    "**/*.{ts,js,tsx,jsx}": [
      "npm run lint"
    ],
    "**/*.{ts,js,tsx,jsx,json,md,yml}": [
      "npm run format:write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aldabil21/react-scheduler.git"
  },
  "keywords": [
    "react",
    "material-ui",
    "calendar",
    "scheduler"
  ],
  "author": "Aldabil",
  "license": "MIT",
  "//homepage": "https://github.com/aldabil21/react-scheduler#readme",
  "bugs": {
    "url": "https://github.com/aldabil21/react-scheduler/issues"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.8",
    "@mui/x-date-pickers": "^5.0.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.24.0",
    "eslint-plugin-react": "^7.31.8",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "ts-loader": "^8.3.0",
    "typescript": "^4.8.4",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0",
    "husky": "^8.0.0"
  },
  "peerDependencies": {
    "@mui/icons-material": ">=5.0.0",
    "@mui/material": ">=5.0.0",
    "@mui/x-date-pickers": ">=5.0.0-alpha",
    "date-fns": ">=2.2",
    "react": ">=17.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
